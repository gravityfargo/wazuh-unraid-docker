<?xml version="1.0"?>
<Container version="2">
  <Name>Wazuh-Indexer</Name>
  <Repository>wazuh/wazuh-indexer:4.8.0</Repository>
  <Registry>https://registry.hub.docker.com/r/wazuh/wazuh-indexer/tags</Registry>
  <Network>br0</Network>
  <MyIP>10.10.10.100</MyIP>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://wazuh.com/</Project>
  <Overview>The Wazuh indexer is a highly scalable, full-text search and analytics engine. This Wazuh central component indexes and stores alerts generated by the Wazuh server and provides near real-time data search and analytics capabilities.</Overview>
  <Category>Security:</Category>
  <WebUI>https://[IP]:[PORT:9200]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/gravityfargo/wazuh-unraid-docker/master/icons/wazuhindexer.png</Icon>
  <ExtraParams>--ulimit memlock=-1:-1 --ulimit nofile=65536:65536</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1719969830</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="OPENSEARCH_JAVA_OPTS" Target="OPENSEARCH_JAVA_OPTS" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">-Xms1g -Xmx1g</Config>
  <Config Name="wazuh-indexer-data" Target="/var/lib/wazuh-indexer" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/wazuh/wazuh-indexer/wazuh-indexer-data</Config>
  <Config Name="root-ca.pem" Target="/usr/share/wazuh-indexer/certs/root-ca.pem" Default="" Mode="ro" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/wazuh/wazuh-indexer/certs/root-ca.pem</Config>
  <Config Name="wazuh.indexer.key" Target="/usr/share/wazuh-indexer/certs/wazuh.indexer.key" Default="" Mode="ro" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/wazuh/wazuh-indexer/certs/wazuh.indexer.key</Config>
  <Config Name="wazuh.indexer.pem" Target="/usr/share/wazuh-indexer/certs/wazuh.indexer.pem" Default="" Mode="ro" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/wazuh/wazuh-indexer/certs/wazuh.indexer.pem</Config>
  <Config Name="admin.pem" Target="/usr/share/wazuh-indexer/certs/admin.pem" Default="" Mode="ro" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/wazuh/wazuh-indexer/certs/admin.pem</Config>
  <Config Name="admin-key.pem" Target="/usr/share/wazuh-indexer/certs/admin-key.pem" Default="" Mode="ro" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/wazuh/wazuh-indexer/certs/admin-key.pem</Config>
  <Config Name="opensearch.yml" Target="/usr/share/wazuh-indexer/opensearch.yml" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/wazuh/wazuh-indexer/opensearch.yml</Config>
  <Config Name="internal_users.yml" Target="/usr/share/wazuh-indexer/opensearch-security/internal_users.yml" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/wazuh/wazuh-indexer/internal_users.yml</Config>
</Container>
